		<servlet-class>jtweet.web.HomeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>web.HomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
		<url-pattern>/replies</url-pattern>
		<url-pattern>/favorites</url-pattern>
		<url-pattern>/retweets_by_me</url-pattern>
