	</servlet-mapping>
	<servlet>
		<servlet-name>web.UserServlet</servlet-name>
		<servlet-class>jtweet.web.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>web.UserServlet</servlet-name>
		<url-pattern>/user/*</url-pattern>
	</servlet-mapping>
	<servlet>
