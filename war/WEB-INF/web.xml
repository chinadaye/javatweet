<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Jtweet</servlet-name>
		<servlet-class>jtweet.JtweetServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Jtweet</servlet-name>
		<url-pattern>/api</url-pattern>
		<!--<url-pattern>/api/*</url-pattern>
	--></servlet-mapping>
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>jtweet.web.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Home</servlet-name>
		<servlet-class>jtweet.web.HomeServlet</servlet-class>
		<init-param>
			<param-name>encrypt_key</param-name>
			<param-value>itwitter</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>Home</servlet-name>
		<url-pattern>/</url-pattern>
		<url-pattern>/home</url-pattern>
		<url-pattern>/sent</url-pattern>
		<url-pattern>/send</url-pattern>
		<url-pattern>/inbox</url-pattern>
		<url-pattern>/reply</url-pattern>
		<url-pattern>/favor</url-pattern>
		<url-pattern>/following</url-pattern>
		<url-pattern>/follower</url-pattern>
		<url-pattern>/public</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>User</servlet-name>
		<servlet-class>jtweet.web.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>User</servlet-name>
		<url-pattern>/user</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Status</servlet-name>
		<servlet-class>jtweet.web.StatusServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Status</servlet-name>
		<url-pattern>/status</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Action</servlet-name>
		<servlet-class>jtweet.web.ActionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Action</servlet-name>
		<url-pattern>/action</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Update</servlet-name>
		<servlet-class>jtweet.web.UpdateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Update</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Setting</servlet-name>
		<servlet-class>jtweet.web.SettingServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Setting</servlet-name>
		<url-pattern>/setting</url-pattern>
		<url-pattern>/setimg</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Twitpic</servlet-name>
		<servlet-class>jtweet.web.TwitpicServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Twitpic</servlet-name>
		<url-pattern>/twitpic</url-pattern>
		<url-pattern>/twitgoo</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Search</servlet-name>
		<servlet-class>jtweet.web.SearchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Search</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PicThumb</servlet-name>
		<servlet-class>jtweet.web.PicThumb</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PicThumb</servlet-name>
		<url-pattern>/picthumb</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Check</servlet-name>
		<servlet-class>jtweet.web.CheckServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Check</servlet-name>
		<url-pattern>/check</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Untinyme</servlet-name>
		<servlet-class>jtweet.web.Untinyme</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Untinyme</servlet-name>
		<url-pattern>/untinyme</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Shorturl</servlet-name>
		<servlet-class>jtweet.web.ShorturlServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Shorturl</servlet-name>
		<url-pattern>/short</url-pattern>
		<url-pattern>/revert</url-pattern>
		<url-pattern>/i/*</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>JSGenerateServlet</servlet-name>
		<servlet-class>jtweet.web.JSGenerateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>JSGenerateServlet</servlet-name>
		<url-pattern>/js_follower_autocomplete</url-pattern>
	</servlet-mapping>
	
	<servlet> 
    <servlet-name>_ah_sessioncleanup</servlet-name> 
    <servlet-class>jtweet.web.SessionCleanupServlet</servlet-class>
 	<!--<servlet-class>com.google.apphosting.utils.servlet.SessionCleanupServlet</servlet-class> 
  --></servlet> 
  <servlet-mapping> 
    <servlet-name>_ah_sessioncleanup</servlet-name> 
    <url-pattern>/_ah/sessioncleanup</url-pattern> 
  </servlet-mapping> 
	
	<error-page>
		<error-code>500</error-code>
		<location>/error.html</location>
	</error-page>
	<error-page>
		<error-code>503</error-code>
		<location>/error.html</location>
	</error-page>
	<error-page>
		<error-code>404</error-code>
		<location>/error.html</location>
	</error-page>
	<error-page>
		<error-code>400</error-code>
		<location>/error.html</location>
	</error-page>
</web-app>
